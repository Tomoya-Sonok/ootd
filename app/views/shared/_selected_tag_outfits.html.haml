.main__lower
    .container
      .main__lower__title
        コーデ一覧
      #outfit-list
        - @outfits.each do |outfit|
          = link_to outfit_path(outfit) do
            .col-md-4#a-outfit
              = image_tag outfit.image.to_s, id: "outfit-image"
              #outfit-tag
                - outfit.tags.each do |tag|
                  = tag.tagname
              #outfit-name